/* latin-ext */
@font-face {
  font-family: 'Lato2';
  font-style: normal;
  font-weight: normal;
  src: url('file:///<%= Rails.root.join('app', 'assets', 'font', 'Lato-Regular.ttf') %>') format('truetype');
  unicode-range: U+0100-024F, U+1E00-1EFF, U+20A0-20AB, U+20AD-20CF, U+2C60-2C7F, U+A720-A7FF;
}

@font-face {
  font-family: 'LatoBold';
  font-style: normal;
  font-weight: normal;
  src: url('file:///<%= Rails.root.join('app', 'assets', 'font', 'Lato-Bold.ttf') %>') format('truetype');
  unicode-range: U+0100-024F, U+1E00-1EFF, U+20A0-20AB, U+20AD-20CF, U+2C60-2C7F, U+A720-A7FF;
}

@font-face {
