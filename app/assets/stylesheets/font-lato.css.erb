/* latin */
@font-face {
  font-family: 'Lato';
  font-style: normal;
  font-weight: normal;
  src: url('file:///<%= Rails.root.join('app', 'assets', 'font', 'MDadn8DQ_3oT6kvnUq_2r_esZW2xOQ-xsNqO47m55DA.woff2') %>') format('woff2')
  unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2212, U+2215, U+E0FF, U+EFFD, U+F000;
}

@font-face {
    font-family: 'LatoWebBlack';
    src: url('file:///<%= Rails.root.join('app', 'assets', 'font','Lato-Black.eot') %>'); /* IE9 Compat Modes */
    src: url('file:///<%= Rails.root.join('app', 'assets', 'font','Lato-Black.eot?#iefix') %>') format('embedded-opentype'),
         url('file:///<%= Rails.root.join('app', 'assets', 'font','Lato-Black.woff2') %>') format('woff2'),
         url('file:///<%= Rails.root.join('app', 'assets', 'font','Lato-Black.woff') %>') format('woff'),
         url('file:///<%= Rails.root.join('app', 'assets', 'font','Lato-Black.ttf') %>') format('truetype');
    font-style: normal;
    font-weight: normal;
    text-rendering: optimizeLegibility;
}
