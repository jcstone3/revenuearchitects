           <%@all_sections.each_with_index do |section,i| %>
              <tr>
                <td><%= section.name.try(:titleize)%></td><td><span class="tblendstyle">
                   <%= @section_questions[i].question_attempted%>/
                   <%= @section_questions_total[i].question_total %>
