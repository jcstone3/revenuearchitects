<%#= render :partial=>"final_report"%>
<% content_for :top_bar_content do  -%> 
        <%= render "sections_nav"%>
<% end -%> 

<% content_for :main_content do -%> 
      <div class="row-fluid">
        <div class="span12">
                  <ul class="nav3-tabs3 usercompare"> 
                                <li class="active"><a href="#overall" data-toggle="tab">Add To Plan Summary</a></li>
                                <li ><a href="#must" data-toggle="tab">Must Do</a></li>
                                <li ><a href="#should" data-toggle="tab">Should Do</a></li>
                                <li ><a href="#could" data-toggle="tab">Could Do</a></li>
                            </ul>
                             
                            <div class="tab-content tab_content_report dash" id="tab-content">
                            <div class="tab-pane active" id="overall" style="height:auto;">
                             <div class="tabHead">
                                  <h1>Add To Plan Summary</h1>
                                   
                                </div>
                                
                                
                                <div class="tabGraph">
                                    <div class="box">  
                                    	<%= render :partial=>"table_header"%>	
                                    	<% @add_to_plan_responses.each do |response|%>
                                            <%= render :partial=>"response_report",
                                               :locals=>{:response => response}%>
                                        <%end%>
                                       <%= render :partial=>"table_end"%> 
                                    </div>
                                </div>
                            </div>

                            
                           <div class="tab-pane" id="must" >
                            <div class="tabHead">
                                  <h1>Must Do</h1>
                                </div>
                                
                                <div class="tabGraph">
                                    <div class="box">
                                        <%= render :partial=>"table_header"%>
                                        <% @must_do_responses.each do |response|%>
                                          <%= render :partial=>"response_report", 
                                          :locals=> {:response =>response} %> 
                                        <%end%>
                                       <%= render :partial=>"table_end"%>
                                    </div>
                                </div>
                           </div>
                 
                 

                            <div class="tab-pane" id="should" >
                            	 <div class="tabHead">
                                  <h1>Should Do</h1>
                                </div>
                                
                                <div class="tabGraph">
                                    <div class="box">  
                                       <%= render :partial=>"table_header"%>
                                        <% @should_do_responses.each do |response|%>
                                          <%= render :partial=>"response_report", 
                                          :locals=> {:response =>response} %> 
                                        <%end%>
                                       <%= render :partial=>"table_end"%>                                      
                                        <%#= render :partial=>"report_response_table", 
                                        :object=> {:responses =>@should_do_responses} %> 
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="could" > 
                            	<div class="tabHead">
                                  <h1>Could Do</h1>
                                   
                                </div>
                                
                                <div class="tabGraph">
                                    <div class="box">                                        
                                        <%= render :partial=>"table_header"%>
                                        <% @could_do_responses.each do |response|%>
                                          <%= render :partial=>"response_report", 
                                          :locals=> {:response =>response} %> 
                                        <%end%>
                                       <%= render :partial=>"table_end"%>         
                                    </div>
                                </div></div>
                           
        </div>
      </div>
    </div> 
<% end %>  

		  