<% content_for :main_content do -%>
      <div class="row-fluid">
        <%= render :partial=>"title.html.slim"%>
        <div class="row-fluid">
          <div class="span12">
            <div class="content-gray">
              <ul class="list-tab nav3-tabs3 usercompare">
                <li>
                  <a data-toggle="tab" href="#all">ALL</a>
                </li>
                <li>
                  <a data-toggle="tab" href="#overall">PRIORITIES</a>
                </li>
                <li>
                  <a data-toggle="tab" href="#must">PLAN SUMMARY</a>
                </li>
                <li class="active on2">
                  <%= link_to(compare_path(params[:id])) do %>
                </i>&nbsp;COMPARE
                  <% end %>
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div class="container-fluid not_margin">
          <div class="span12" style="margin: 0px">
            <ul class="list-tab nav3-tabs3 usercompare">
                <li class="active"><a href="#overall" data-toggle="tab">OVERALL</a></li>
                <li><%= link_to(compare_strategy_url(params[:id])) do %>
                    STRATEGY
                     <% end %>
                 </li>
                 <li><%= link_to(compare_system_url(params[:id])) do %>
                    SYSTEMS
                     <% end %>
                 </li>
                 <li><%= link_to(compare_programs_url(params[:id])) do %>
                    PROGRAMS
                     <% end %>
                 </li>

            </ul>
            <div class="tab-content3 dash" style="height:auto;">
              <div class="tab-pane active" id="overall">
                  <div class="tabGraph">
                      <div class="box_compare">
                          <h1>Response of the month :: <strong><%= @survey.created_at.strftime("%B  %Y")%></strong></h1>

                         <div id='chart'></div>
                          <%= render_chart(@chart, 'chart') %>

                      </div>
                          <!--table goes here-->
                       <div class="container report-top">
                          <div class="row-fluid">

                           <%= render :partial=>"compare_response_table"%>

                          </div>
                        </div>
                    <!--end table-->

                  </div>
              </div>
            </div>
            </br>
        </div>
      </div>
    </div>
<% end %>
