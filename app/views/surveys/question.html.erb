<% content_for :top_bar_content do  -%> 
        <%= render "sections"%>
<% end -%> 

 <%content_for :main_content do%>
    <div class="content">
       <h1 class="question">Q<%= @question.id %>. <%= @question.name%></h1>
       
            <%= form_for(@response, :url=>reponses_url) do |f|%> 
                 <%= f.hidden_field :survey_id, :value=>@survey.id%>
                 <%= f.hidden_field :question_id, :value=>@question.id%> 
                 <%= render "questions_set", :f => f%>
            <%end%>
              <%= render "question_details"%>
        </div> 
<% end %>

<% content_for :side_content do -%>
    <%= render "question_side_details" -%> 
<% end  -%>  

          
         
