
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :validate => true, :html => {:class => "reset" }) do |f| %>
  <%#= devise_error_messages! %>
      <h3 style="margin-top:-12px;">Full Name<span>*</span></h3>
      <%= f.text_field :username,  :class=>"span3 righttextBox", :maxlength => "30",  :placeholder=>"Full Name", :validate =>{:presence => true} %>

      <h3>Email<span>*</span></h3>

      <%= f.email_field :email, :class=>"span3 righttextBox" ,:placeholder=>"test@gmail.com", :validate =>
                        {:presence => true, :uniqueness => true}  %>
      <h3 >Password<span>*</span></h3>
      <%= f.password_field :password,  :class=>"span3 righttextBox", :maxlength => "30", :placeholder=>"Password for the site", :validate =>{:presence => true} %>
      <label class="checkbox check1">
      <input type="checkbox" checked> <p class="checkNotice">Please send me updates and tips from Revenue Grader. <%= link_to 'Privacy Policy', privacy_policy_url%></p>
      </label>
      <%= f.submit "Sign up and Get Started!", :class=>"btn btn-primary btn-large btnStart" %>
      <h5 class="confidential">
        We will keep your personal information strictly confidential
      </h5>
      <%= hidden_field_tag('login', "no")%>
<% end %>



 