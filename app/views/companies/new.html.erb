		<div class="container" style="margin-top:20px;">
			<div class="row-fluid">
				<div class="span8 leftBox" style="height:auto;background-color:#ffffff;padding-left:8px">
                    <div class="head">
                        <h3>
                            Company Details
                        </h3>
                       
                    </div><br/>
                    <div style="padding-left:20px;">
                   <%= form_for(@company) do |f| %>
                    

                  <h3><%= f.label :name %></h3>
                  <%= f.text_field :name, :validate =>{:presence => true} %><br>

                  <h3><%= f.label :website %></h3>
                  <%= f.email_field :website, :validate => { :presence => true, :uniqueness => false}  %><br>

                  <h3><%= f.label :industry %></h3>
                  <%= f.password_field :industry_id, :validate => { :presence => true, :confirmation => false} %><br>

                  <%#= f.label :password_confirmation %><br />
                  <%#= f.password_field :password_confirmation %>
                  <a href="#" class="btn btn-large btn-info" style="float:left; margin-right:20px;margin-bottom:20px;"><i class="icon-folder-close icon-white"></i> Continue to Survey</a>
                  <!--<%= f.submit "Continue to Survey",cls:"btn btn-info"%>-->
                <% end %>

				
                 </div>  
                    
                </div>

				<div class="span3 rightBox" >
                    <div class="scoreBoard" style="margin-top:2px;">
                        <div class="head">About Revenue Grader </div>
