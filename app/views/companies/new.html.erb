<% content_for :main_content do -%>
  <div class="content">
    <div class="span5 ">
       <h1 class="question">Company Details</h1>
      <p class="question">Please provide basic information about your company</p>
          
      <%= simple_form_for(@company, html: { validate: true, novalidate: false}) do |f| %>

        <div class="control-group string required <%= ( @user.errors[:email].empty?)? '' : 'error' -%>">
          <label class="string required control-label " for="user_email">
          <abbr title="required">*</abbr>User Email</label>
          <div class="controls">
            <%= text_field(:user, :email, value: @user.email, required: true)-%>
            <% unless (@user.errors[:email].empty?) -%>
              <span class="help-inline"><%= @user.errors[:email].first -%></span>
            <% end -%>
          </div>
        </div>
        <div class="control-group string required <%= ( @user.errors[:username].empty?)? '' : 'error' -%>">
          <label class="string required control-label " for="user_username">
          <abbr title="required">*</abbr>User Name</label>
          <div class="controls">
            <%= text_field(:user, :username, value: @user.username, required: true )-%>
            <% unless (@user.errors[:username].empty?) -%>
              <span class="help-inline"><%= @user.errors[:username].first -%></span>
            <% end -%>
          </div>
        </div>

        <%= f.input :name, 
                    :label => "Your Company Name",  
                    :input_html => { :class => 'input-xlarge', required: true }, 
                    :label_html => { :class => '' } -%> 
        <%= f.input :website,  
                    :input_html => { :class => 'input-xlarge', required: true } -%>
        <%= f.association :industry, 
                      :prompt => "Select Industry",  
                      :input_html => { :class => 'input-xlarge', required: true }   -%>  

         <div class="form-actions1">
            <%= f.button :submit, "Continue to Diagnostic", :class => 'btn-primary' %>
          </div>
      <% end -%> 
      <script type="text/javascript">
