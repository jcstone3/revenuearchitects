<div class="container" style="margin-top:20px;">
      <div class="row-fluid">
        <div class="span12">
                  <ul class="nav2 nav2-tabs2" style="margin: 0 0 0px 0px;padding: 0;"> 
                                <li><a href="#settings" data-toggle="tab">Settings</a></li>
                                <li><a href="#questions" data-toggle="tab">Questions</a></li>
                                <li><a href="#sections" data-toggle="tab">Sections</a></li>
                                <li><a href="#reports" data-toggle="tab">Reports</a></li>
                                <li class="active"><%= link_to 'Users', admin_users_url %></li>
                                <!--<li><a href="" data-toggle="tab">Users</a></li> -->
                                <li><%= link_to 'Dashboard', admin_root_url %></li>
                            </ul>
                             
                            <div class="tab-content"style="padding-top:0px">
                            <div class="tab-pane active" id="dashboard" style="height:730px;">
                <div class="tabHead">
                                  <h1>Dashboard</h1>
                                    <h2>Overview about the user activity</h2>
                                </div>
                                <div class="tabMid">
                                  <div class="activeUsers">
                                        <h3>20</h3>
                                        <h4>Active Users</h4>
                                    </div>
                                    <div class="responses">
                                        <h3>50</h3>
                                        <h4>Responses</h4>
                                    </div>
                                    <div class="average">
                                        <h3>12%</h3>
                                        <h4>Average Percentage</h4>
                                    </div>
                                    <div class="button-bar">
                                    <div class="btn-group"  style="margin-top: 45px; width: 150px;">
                                          <a href="#" class="btn"><i class="icon-calendar"></i> This Month</a>
                                          <a href="#" data-toggle="dropdown" class="btn dropdown-toggle"><span class="caret"></span></a>
                                          <ul class="dropdown-menu">
                                            <li><a href="#"><i class="icon-calendar"></i> Last Month</a></li>
                                            <li><a href="#"><i class="icon-calendar"></i> Last Year</a></li>
                                          </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="tabGraph">
                                    <div class="box">                                        
                                        <h1>Responses in the current month <strong>March 2012</strong></h1>
                                        <h1>List of users</h1>
                                        <table >
                                           <tr><td>Name</td><td>Email</td><td colspan="2">Actions</td></tr>
                                            <tr>&nbsp;</tr>
                                            <tr>&nbsp;</tr>
                                          <%@users.each do |user|%>
                                          <tr>
                                          <td><%= user.username%></td>
                                          <td><%= user.email%></td>
                                         <td>Activate/Deactivate</td>
                                          </tr>
                                          <%end%>
                                          
                                      </table>
                                      <div style="background-color:black;">
                                      <%= will_paginate(@users)%>
                                      <div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="users" style="height:500px;">Users</div>
                            <div class="tab-pane" id="reports" style="height:500px;">Reports</div>
                            <div class="tab-pane" id="sections" style="height:500px;">Sections</div>
                            <div class="tab-pane" id="questions" style="height:500px;">Questions</div>
                            <div class="tab-pane" id="settings" style="height:500px;">Settings</div>
                            </div>
        </div>
      </div>
    </div> 