<div class="container" style="margin-top:20px;">
            <div class="row-fluid">
                <div class="span12">
                            <ul class="nav2 nav2-tabs2" style="margin: 0 0 0px 0px;padding: 0;">
                                <li class="active"><%= link_to 'Questions'%></a></li>
                                <li><%=link_to 'Sections', admin_sections_url%>
                                </li>
                                <li><%= link_to 'Users', admin_users_url%></li>
                                 <li><%= link_to 'Dashboard', admin_root_url %></li>
                            </ul>
                             
                            <div class="tab-content"style="padding-top:0px">
                            <div class="tab-pane active" id="dashboard" style="height:730px;">
                                <div class="tabHead">
                                        <h1>Questions</h1>
                                        <h2>Questions for the Survey</h2>
                                </div>
                                
                                <div class="tabGraph">
                                     <div id="box">
                                  <table cellpadding="1" cellspacing="1" id="user_table" class="report-body dataTable2"> 
                                  <thead>
                                    <tr>
                                      <th><h3>Name</h3></th>
                                      <th><h3>Description</h3></th>
                                      <th><h3>Section :: SubSection</h3></th>
                                      <th><h3>Points</h3></th>                                     
                                    </tr>
                                  </thead>
                                  <tbody>
                                    <% @questions.each do |question| %>
                                        <tr>
                                            <td><%= question.name %></td>
                                            <td><%= question.description %></td>
                                           
                                            <td><%= question.section_name %> :: <%= question.subsection_name%></td>
                                            <td><%= question.points %></td>            
                                        </tr>
                                            <% end %>

                                          </tbody>        
                                </table> 
                                <br />
                                <p class="btn-admin"><%= link_to 'Add Question', admin_new_questions_url, :class=>"btn-adminprimary" %></p>    
                                </div>
                                </div>
                            </div>                                    
                                        
                                        
                                    
                                </div>
                            </div>
                            <div class="tab-pane" id="users" style="height:500px;">Users</div>
                            <div class="tab-pane" id="reports" style="height:500px;">Reports</div>
                            <div class="tab-pane" id="sections" style="height:500px;">Sections</div>
                            <div class="tab-pane" id="questions" style="height:500px;">Questions</div>
                            <div class="tab-pane" id="settings" style="height:500px;">Settings</div>
                            </div>
                </div>
            </div>
        </div> 