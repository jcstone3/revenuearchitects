<div class="container left" style="margin-top:20px;">
            <div class="row-fluid">
                <div class="span12">
                            <ul class="nav2 nav2-tabs2" style="margin: 0 0 0px 0px;padding: 0;">
                                <li><%=link_to 'Feedbacks', admin_feedbacks_url%>
                                </li>
                                <li class="active"><%= link_to 'Questions'%></a></li>
                                <li><%=link_to 'Sections', admin_sections_url%>
                                </li>
                                <li><%= link_to 'Users', admin_users_url%></li>
                                 <li><%= link_to 'Dashboard', admin_root_url %></li>
                            </ul>
                             
                            <div class="tab-content2"style="padding-top:0px">
                            <div class="tab-pane active" id="dashboard" >
                                <div class="tabHead">
                                        <h1>Questions</h1>
                                        <h2>Questions for the Survey</h2>
                                </div>
                                
                                <div class="tabGraph">
                                     <div id="box">
                                     <div class="quesMain" style="height:auto;">
                                            <div class="addQuesHead">
                                                <h1>Add New Question</h1>
                                            </div>
                                            <div class="rightboxName">
                                                <%= form_for @question, :url=>{:action => :create}, :validate=>true do |f| %>
                                            <h3>Question<span class="star">*</span></h3>
                                           <%= f.text_area :name, :class=>"input-xlarge", 
                                           :placeholder=>"Type new question here...", :style=>"margin-bottom:9px;", :rows=>3, :cols=>8%>
                                          <h3>Description<span class="star">*</span></h3>
                                           <%= f.text_area :description, :class=>"input-xlarge", 
                                           :placeholder=>"Type description for question here...", :style=>"margin-bottom:9px;", :rows=>3, :cols=>8%>
                                             <%= f.hidden_field :is_active, :value=>true%>
                                            <h3>Section<span class="star">*</span></h3>
                                            <%= f.select "sub_section_id", options_for_select(@subsection.collect{|x| [x.name,x.id]})%>
                                            
                                            <h3>Points<span class="star">*</span></h3>
                                            <%= f.select :points, [5,10,20]%>
                                            <br/>
                                            
                                                <%= f.submit 'Save', :class=>"btn btn-primary"%>
                                            <br/>
                                            
                                            <%#= f.submit 'Save & New', :class=>"btn ",:style=>"margin-left:30px;"%>
                                            <%#= f.submit 'Reset', :class=>"btn",:style=>"margin-left:30px;"%>
                                               
                                            </div>
                                            <%end%>
                                            </div>   
                                </div>
                                </div>
                            </div>                                    
                                        
                                        
                                    
                                </div>
                            </div>
                            <!-- <div class="tab-pane" id="users" >Users</div>
                            <div class="tab-pane" id="reports" >Reports</div>
                            <div class="tab-pane" id="sections" >Sections</div>
                            <div class="tab-pane" id="questions">Questions</div>
                            <div class="tab-pane" id="settings">Settings</div> -->
                            </div>
                </div>