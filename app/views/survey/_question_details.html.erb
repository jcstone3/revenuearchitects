                    <div class="qdots" id="qdots"> 
                      <div class="pagination" >
                        <ul>    
                            <%if(params[:question_id].to_i == 1)%>                     
                                <li class="disabled">                              
                                  <%= link_to 'Prev',:style=>"border:0px none"%>
                                </li>  
                            <%else%>
                                <li>
                                 <%= link_to 'Prev', questions_url(params[:id],
                                    params[:question_id].to_i - 1)%>
                                 </li>   
                             <%end%>
                            
                            <% @questions.each do |question|%>
                                <% if (question.id == params[:question_id].to_i) %>
                                    <li>
                                      <%= link_to questions_url(params[:id],question.id) do%>
                                      <i class="icon-ok-circle icon-white"></i>
                                      <%end%>
                                    </li>
                                 <%elsif (question.id < params[:question_id].to_i)%>       
                                    <li>
                                      <%= link_to questions_url(params[:id],question.id) do%>
                                      <i class="icon-ok icon-white"></i>
                                      <%end%>
                                    </li>
                                <%else%>
                                    <li>
                                      <%= link_to questions_url(params[:id],question.id) do%>
                                      <i class="icon-ban-circle"></i>
                                      <%end%>
                                    </li>
                                <%end%>
                            <%end%>
                             <%if(params[:question_id].to_i == @question_all)%>
                               <li class ="disabled">
                                <%= link_to 'Next'%>
                              </li>
                             <%else%> 
                              <li>
                                <%= link_to 'Next', questions_url(params[:id],params[:question_id].to_i + 1)%>
                              </li>
                            <%end%>
                        </ul>
                      </div>
                    </div>
                </div>
                <div class="span3 rightBox" style="height:auto;">
                    <div class="processIcons">
                        <span>
                        <%= image_tag "qa.png",:width=>"47px",:height=>"49px" %>
                        <p style="color:#525c62;">Q&amp;A</p>
                        </span>
                        <span>
                        <%= image_tag "processing.png",:width=>"47px",:height=>"49px" %>
                        <p style="color:#c56d2b;">Demographics</p>
                        </span>
                        <span>
                        <%= image_tag "download.png",:width=>"47px",:height=>"49px",:style=>"margin-left: 26px;" %>
                        <p style="color:#116094; margin-left: 30px">Report</p>
                        </span>
                    </div>
                    <div class="scoreBoard">
                        <div class="head" >
                          <p style="margin:0px;padding-left:5px">Total score for this section</p> </div>
                            <div class="score"><br>
                           <%= @total_score%>/<span style="font-size:30px;"><%= @section.total_points%>
                            </span> <br>
                            <span>POINTS</span>

                            </div>
                    </div>

