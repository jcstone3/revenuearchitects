                      <span class="green"><%= @sections[0].total_points + @sections[1].total_points + @sections[2].total_points%></span>  <i class="icon-chevron-right icon-white"></i>                    
                     <%end%>
                    </li>                 

                    <%#= hidden_field_tag('section', @section.id)%>
                                        
                  </ul>

            </div>
