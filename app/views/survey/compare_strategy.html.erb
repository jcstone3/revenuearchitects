</div>
<div class="container left">
      <div class="row-fluid">
        <div class="span12">
                  <ul class="nav3-tabs3 usercompare"> 
                                <li><%= link_to(compare_url(params[:id])) do %>
                                    Overall
                                     <% end %> 
                                 </li>
                                <li  class="active" href="#strategy" data-toggle="tab"><%= link_to(compare_strategy_url(params[:id])) do %>
                                    Strategy
                                     <% end %> 
                                 </li>
                                 <li><%= link_to(compare_system_url(params[:id])) do %>
                                    System
                                     <% end %> 
                                 </li>
                                 <li><%= link_to(compare_programs_url(params[:id])) do %>
                                    Programs
                                     <% end %> 
                                 </li>
                            </ul>
                             
                            <div class="tab-content3 dash"  style="height:auto;">
                            <div class="tab-pane active userTable" id="strategy" >
                <div class="tabHead">
                                  <h1>Strategy</h1>
                                   
                                </div>
                                
                                <div class="tabGraph">
                                    <div class="box">                                        
                                        <h1>Responses in the current month <strong>March 2012</strong></h1>
                                        <div class="graph">
                              <img src="<%= @line_graph_strategy %>"/>
             
             
                                         </div>
                                    </div>
                                     <!--table goes here-->
                                     <div class="container report-top">
                                        <div class="row-fluid">
                                       
                                          <table cellpadding="1" cellspacing="1" id="resultTable" class="report-body dataTable2"> 
                                                <thead>
                                                  <tr>
                                                    <th><h3>Questions</h3></th>
                                                    <th><h3>Group</h3></th>
                                                    <th><h3>Score</h3></th>
                                                    <th><h3>In Plan</h3></th>
                                                    <th><h3>Priority</h3></th>
                                                    <th><h3>Points</h3></th>
                                                  </tr>
                                                </thead>
                                                <tbody>
                                        <% @responses.each do |response| %>
                                            <tr>
                                                <td><%= response.name %></td>
                                                <td><%= response.section_name%><%= response.sub_sect_name%></td>
                                                <td><%= response.answer_1 %></td>
                                                <td><%= "Y" %></td>
                                                <td><%= "Must" %></td>
                                                <td><%= response.points %></td>
                                            </tr>
                                        <% end %>
                                        
                                                </tbody>           
                                          </table>      
                                           
                                        </div>
                                      </div>
                                  <!--end table-->  
                                </div>
                            </div>         
                  </div>

        <!--second tab-->
                        

      </div>
    </div> 
