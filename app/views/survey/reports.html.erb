          <div class="graph" style="background-color:#FFF;">
            
            <img src="<%#= @line_graph %>">
             
          </div>
        </div>

        <div class="content" style="page-break-after:always;">
             <h1>Strategy</h1>
        </div>

        <div class="content" style="page-break-after:always;">
          <div class="graph">
            <h1>Strategy Graph</h1>
            <img src="<%#= @line_graph_strategy %>"/>
              
          </div>
          <div class="textbox">
             <%= render :partial=>"report_response_table"%>            
          </div> 
        </div>


        <div class="content" style="page-break-after:always;">
             <h1>System</h1>
        </div>

        <div class="content" style="page-break-after:always;">
          <div class="graph">
