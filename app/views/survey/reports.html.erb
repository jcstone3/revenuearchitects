            <img src="<%#= @line_graph_system %>"/>
            
          </div>
          <div class="textbox">
            <%= render :partial=>"report_response_table"%>
                      </div> 
        </div>


        <div class="content" style="page-break-after:always;">
             <h1>Programs</h1>
        </div>

        <div class="content" style="page-break-after:always;">
          <div class="graph">
             <h1>Program  Graph</h1>
             <img src="<%#= @line_graph_programs%>"/>
          </div>
          <div class="textbox">
            <%= render :partial=>"report_response_table"%>          
          </div> 
        </div>


        <div class="content" style="page-break-after:always;">
             <h1>Actions</h1>
        </div>

        <div class="content" style="page-break-after:always;">
             <h1>Must Do</h1>
        </div>

        <div class="content" style="page-break-after:always;">
             <h1>Should Do</h1>
        </div>


        <div class="content" style="page-break-after:always;">
             <h1>Could Do</h1>
        </div>

        <div class="content" style="page-break-after:always;">
             <h2>About Revenue Grader</h2>
