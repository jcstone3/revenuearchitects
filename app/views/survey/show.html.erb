    <div class="container" style="margin-top:20px;">
      <div class="row-fluid">
        <div class="span9 leftBox" style="height:auto;background-color:#ffffff">                  
          <div class="question">
            <h1> Existing Surveys </h1>
          </div>

          <div class="text" >
          <p style="margin-left:25px; padding-top:70px;">Please select a survey to continue
          </p>
          </div>
          
         
           <div style="padding-left:0px; padding-top:0px; margin-top:0px; height:220px;">
                     <% current_user.companies.each do |cmpany| %>      
                     <div class="points" style="margin-top:5px; padding-top:0px;" >
                        <div class="option" style="margin-top:5px;padding-top:0px;" >
                        <h3>Surveys of company <%= cmpany.name%></h3>

                         <%@active_survey = cmpany.surveys.find(:all, :conditions=>["is_active=?", true]) %>
                          <% if @active_survey.present?%>
                          <% @active_survey.each do |survey|%>
                              <%= link_to "Survey_#{survey.id}", get_response_status_url(survey)%>
                          <%end%>
                          <% end %>                                                   
                        </div>
                      </div>
                    <%end%>                      
                     
             </div>  
           </div>
           <div class="span3 rightBox" >
               <div class="scoreBoard" style="margin-top:2px;">
                  <div class="head">About Revenue Grader </div>
                     <div class="score">
                        <span>Some notes on revenue grader</span>
                     </div>
                  </div>
                  <div class="scoreBoard">
                     <div class="head">About Revenue Architects </div>
                        <div class="score">
                            <span>Some notes on revenue architects</span>
                        </div>
                     </div>
                  </div>      
                </div>
              </div> 
