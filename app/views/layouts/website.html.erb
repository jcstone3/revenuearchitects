    <![endif]-->
    <link href='https://fonts.googleapis.com/css?family=Lato:400,300' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Poiret+One' rel='stylesheet' type='text/css'>
    <!-- Le styles -->
    <link rel="icon" type="image/png" href="/assets/rg_icon.png" />
    <%= stylesheet_link_tag "application", :media => "all" %>

    <!-- Le fav and touch icons -->
    <link href="images/apple-touch-icon.png" rel="apple-touch-icon">
    <link href="images/apple-touch-icon-72x72.png" rel="apple-touch-icon" sizes="72x72">
    <link href="images/apple-touch-icon-114x114.png" rel="apple-touch-icon" sizes="114x114">

  </head>

  <body>
    <div class="container-fluid" >
      <div class="row-fluid top-bar">
        <%= render  "/layouts/shared/header" -%>
      </div><!-- / end of row -->

      <% if !flash.blank? -%>
        <div class="row-fluid">
          <% flash.each do |key, value| %>
            <%= content_tag :div, :id=>'flash_msg', :class => "alert alert-#{key}" do %>
              <%= value unless value.blank? %>
              <a class="close" data-dismiss="alert">Ã—</a>
            <% end %>
          <% end %>
        </div><!-- / -->
      <% end -%>

      <div class="row-fluid content-section landing" >

        <div class="span4 side-content">
          <div class="main-content-header">
            <div class="heading">
              <h1 style="margin-top:30px;">Assess your <br />
              Revenue Potential <%= link_to 'now', new_user_session_path %>
              </h1>
            </div><!-- / -->
            <div class="arrow">
              &nbsp;
            </div><!-- / -->
          </div><!-- / -->
          <%= yield :side_content -%>
        </div><!-- / end of span4 - side-content-->

        <div class="row-fluid main-content clear">
          <%= yield :main_content %>
        </div> <!-- / end of span8 - main -->

      </div> <!-- /end of content-section -->

      <div class="row-fluid footer">
        <%= render '/layouts/shared/footer'%>
      </div> <!-- / End of footer -->
    </div> <!-- end on container - main -->

    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->

    <%= javascript_include_tag "application" %>
    <%= feedback_tab(:position => 'left') %>
